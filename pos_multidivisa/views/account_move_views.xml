<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_account_move_form_inherit_withholding" model="ir.ui.view">
            <field name="name">account.move.form.withholding</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <!-- Insert withholding rate and fiscal book into invoice form -->
                <xpath expr="//sheet/notebook/page[1]/group" position="inside">
                    <group string="Retenciones">
                        <field name="withholding_rate"/>
                        <field name="fiscal_book_id"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
